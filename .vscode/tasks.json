{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Config Windows x64 Debug",
            "type": "process",
            "command": "cmake",
            "args": [
                "--preset",
                "windows-x64-debug",
            ],
        },
        {
            "label": "Build Windows x64 Debug",
            "type": "process",
            "command": "cmake",
            "args": [
                "--build",
                "build/windows-x64-debug",
                "--target",
                "install",
            ],
            "dependsOn": [
                "Config Windows x64 Debug",
            ]
        },
        {
            "label": "Config Windows x64 Release",
            "type": "process",
            "command": "cmake",
            "args": [
                "--preset",
                "windows-x64-release",
            ]
        },
        {
            "label": "Build Windows x64 Release",
            "type": "process",
            "command": "cmake",
            "args": [
                "--build",
                "build/windows-x64-release",
                "--target",
                "install",
            ],
            "dependsOn": [
                "Config Windows x64 Release",
            ]
        },
    ]
}
